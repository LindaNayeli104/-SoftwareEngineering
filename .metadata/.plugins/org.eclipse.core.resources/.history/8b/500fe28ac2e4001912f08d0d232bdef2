
public class Vertex {
	private double x, y;
	
	public Vertex() {
		this.x = 0.0;
		this.y = 0.0;
	}
	
	public Vertex(double x, double y) {
		this.x = x;
		this.y = y;
	}
	
	public Vertex(Vertex v) {
		this.x = v.x;             
		this.y = v.y;
	}
	
	public double get_x() {
		return x;
	}
	
	public double get_y() {
		return y;
	}


	public void set_x(double x) {
		this.x = x;
	}


	public void set_y(double y) {
		this.y = y;
	}
	
	
	
}
